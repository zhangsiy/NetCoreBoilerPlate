{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Literate", "W4k.Serilog.Sinks.SumoLogic", "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Fatal",
        "System": "Warning"
      }
    },
    "WriteTo": [
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "NetCoreSample"
    }
  },

  "ServiceDependencies": {
    "MyProductServiceBaseUrl": "www.foo.bar"
  }
}